@using CmsWeb
@{
    Layout = "/Views/Shared/Reports.cshtml";
    ViewBag.Title = "Contribution Statements";
}
@section scripts
{
    <script>
        $(function() {
            $(".datepicker").datepicker();
            $("a.bt").button();
        });
    </script>
}
<!DOCTYPE html>
<html>
<head>
    <title>Find Duplicates</title>
</head>
<body>
    <h2>
        Find Duplicates</h2>
    <a href="/">Home</a>
    <form action="/Duplicates/Find" method="post">
    <table>
        <tr> <th> Created on or after date: </th> <td> @Html.DatePicker("FromDate")</td> </tr>
        <tr> <th> Created on or before date: </th> <td> @Html.DatePicker("ToDate")</td></tr>
        <tr> <td colspan="2" class="center"> <input type="submit" class="bt" name="Submit" value="Run" /></td></tr>
    </table>
    </form>
</body>
</html>
