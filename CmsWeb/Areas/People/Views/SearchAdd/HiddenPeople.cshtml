@using UtilityExtensions
@model CmsWeb.Areas.People.Models.SearchAddModel
@for (var n = 0; n < Model.List.Count; n++)
{
    var p = Model.List[n];
    p.index = n;
    //@Html.Partial("HiddenPerson", p)
<input type="hidden" name="List.Index" value="@p.index" />
<input type="hidden" name="List[@p.index].PeopleId" value="@p.PeopleId" />
<input type="hidden" name="List[@p.index].FamilyId" value="@p.FamilyId" />
<input type="hidden" name="List[@p.index].First" value="@p.First" />
<input type="hidden" name="List[@p.index].Goesby" value="@p.GoesBy" />
<input type="hidden" name="List[@p.index].Middle" value="@p.Middle" />
<input type="hidden" name="List[@p.index].Last" value="@p.Last" />
<input type="hidden" name="List[@p.index].dob" value="@p.birthday.FormatDate()" />
<input type="hidden" name="List[@p.index].Email" value="@p.Email" />
<input type="hidden" name="List[@p.index].Phone" value="@p.Phone" />
<input type="hidden" name="List[@p.index].Title" value="@p.Title" />
<input type="hidden" name="List[@p.index].Suffix" value="@p.Suffix" />
<input type="hidden" name="List[@p.index].Marital" value="@p.Marital.Value" />
<input type="hidden" name="List[@p.index].Gender" value="@p.Gender.Value" />
<input type="hidden" name="List[@p.index].HomePhone" value="@p.HomePhone" />
<input type="hidden" name="List[@p.index].Address" value="@p.Address" />
<input type="hidden" name="List[@p.index].Address2" value="@p.Address2" />
<input type="hidden" name="List[@p.index].Zip" value="@p.Zip" />
<input type="hidden" name="List[@p.index].City" value="@p.City" />
<input type="hidden" name="List[@p.index].State.Value" value="@p.State.Value" />
<input type="hidden" name="List[@p.index].Country.Value" value="@p.Country.Value" />
<input type="hidden" name="List[@p.index].Campus.Value" value="@p.Campus.Value" />
<input type="hidden" name="List[@p.index].EntryPoint.Value" value="@p.EntryPoint.Value" />
<input type="hidden" name="List[@p.index].context" value="@p.context" />
}