@model string
@{
    var help = Html.HelpMessageFor(m => m, "help-block");
}
<div class="control-group @help.errorClass">
    @Html.LabelFor(m => m, new { @class = "control-label"})
    <div class="controls">
        @Html.TextBoxFor(m => m, new { @class = "ajax-typeahead", data_link = ViewBag.link })@Html.Raw(help.message)
    </div>
</div>
